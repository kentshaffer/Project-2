<div class='container container-fluid'>
  <div class='flex-row justify-center'>
    <section class='card'>
        <div class='card-body'>
            <p id={{id}}>{{goal_name}} {{#unless goal.goal_open}}✔️{{/unless}}</p>
        </div>
    </section>
    {{#each todos as |todo|}}
    {{> todos}}
    {{/each}}
  </div>
</div>